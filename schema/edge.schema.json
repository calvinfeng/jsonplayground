{
  "$schema": "https://json-schema.org/draft/2019-09/schema",
  "title": "Edge",
  "description": "directed edge of a directed acyclic graph",
  "type": "object",
  "properties": {
    "id": {
      "type": "integer",
      "minimum": 1
    },
    "src": {
      "type": "integer",
      "minimum": 1
    },
    "dest": {
      "type": "integer",
      "minimum": 1
    }
  },
  "required": ["id", "src", "dest"]
}